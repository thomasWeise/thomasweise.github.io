{%- assign outarray = "" | split: "," -%}
{%- for person in include.persons -%}
    {%- assign personstr=site.data.people[person] -%}
    {%- assign personarray=personstr | split: "äöü@" -%}
    {%- assign outarray = outarray | concat: personarray -%}
{%- endfor -%}
{%- include print_array.liquid array=outarray -%}
